{
    "manifest_version": 2,
    "name": "FlipSide Views",
    "description": "Opens new tab(s) with an alternative point of view!",
    "version": "1.1",

    "browser_action": {
        "default_title": "FlipSide Views",
        "default_popup": "Popup.html",
        "default_icon": "Flipside.png"
    },

    "permissions": [
        "http://*/*", 
        "https://*/*",
        "tabs",
        "bookmarks",
        "browsingData",
        "tabCapture"
    ]
}