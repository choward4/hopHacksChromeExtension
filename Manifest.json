{
    "manifest_version": 2,
    "name": "FlipSide",
    "description": "This is the description. WOO. :D",
    "version": "1.0.0",

    "browser_action": {
        "default_title": "This is the default title",
        "default_popup": "Popup.html"
    },

    "permissions": [
        "http://*/*", 
        "https://*/*",
        "tabs",
        "bookmarks",
        "browsingData",
        "tabCapture"
    ]
}